<suite name="CRIP Automation Script">

    <test name="com.UITAP Tests">

        <parameter name="useCloudEnv" value="false"></parameter>
        <parameter name="envName" value="browserstack"></parameter>
        <parameter name="os" value="windows"></parameter>
        <parameter name="osVersion" value="11"></parameter>
        <parameter name="browserName" value="chrome"/>
        <parameter name="browserVersion" value="102"/>
        <parameter name="url" value="http://uitestingplayground.com/"/>

        <classes>
            <class name="uitaptests.ExistingElement"></class>
            <class name="uitaptests.GetValueOrLoad"></class>
            <class name="uitaptests.HyperLinks"></class>
            <class name="uitaptests.Login"></class>
            <class name="uitaptests.PageButtons"></class>
            <class name="uitaptests.PageRedirects"></class>
            <class name="uitaptests.PopUps"></class>
            <class name="uitaptests.ScrollBar"></class>
            <class name="uitaptests.TextField"></class>
            <class name="uitaptests.ZTestRun"></class>
        </classes>

    </test>

